<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.primointeractive.tnplpro" version="1.1">
<preference name="phonegap-version" value="3.5.0" />
<name>TNPL Pro</name>
<description>TNPL Pro, the enhanced official Nuts Poker League app, enables you to check current and past league tables, view individual event players and winners, find your nearest venues and keep up-to-date with the latest news from the league.</description>
<author href="http://www.primointeractive.com" email="studio@primointeractive.com">Primo Interactive</author>
<preference name="orientation" value="default" />
<preference name="target-device" value="universal" />
<preference name="fullscreen" value="false" />
<preference name="stay-in-webview" value="true" />
<preference name="exit-on-suspend" value="true" />
<preference name="show-splash-screen-spinner" value="false" />
<preference name="splash-screen-duration" value="3000" />
<preference name="disable-cursor" value="true" />
<icon src="icon.png" />
<icon src="icons/ios/57.png" width="57" height="57" />
<icon src="icons/ios/72.png" gap:platform="ios" width="72" height="72" />
<icon src="icons/ios/114.png" width="114" height="114" />
<gap:splash src="splash.png" />
<gap:splash src="splash/ios/320x480.png" width="320" height="480" />
<gap:splash src="splash/ios/640x960.png" width="640" height="960" />
<gap:splash src="splash/ios/640x1136.png" width="640" height="1136" />
<gap:splash src="splash/ios/1024x768.png" width="1024" height="768" />
<gap:splash src="splash/ios/768x1024.png" width="768" height="1024" />
<preference name="permissions" value="none"/>
<access origin="http://tnplmobile.com" subdomains="true" />
<access origin="http://thenutspokerleague.com" subdomains="true" />
<access origin="http://google.com" subdomains="true" />
<access origin="http://fbcdn.net" subdomains="true" />
</widget>